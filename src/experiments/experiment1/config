            ]

            routingTable = [
                ["10.10.1.0", "255.255.255.252", "10.10.1.2", "lp2", 0],
                ["10.10.2.0", "255.255.255.252", "10.10.2.1", "lp3", 0],

             //   ["10.10.3.0", "255.255.255.252", "10.10.2.2", "lp3", 1],
             //   ["10.10.3.0", "255.255.255.252", "10.10.1.1", "lp2", 2],
